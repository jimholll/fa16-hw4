<h1> This is working, hooray! </h1>

<h2> Cats </h2>
<table>
	<tr>
		<td>Name</td>
	</tr>
	<% @cat.each do |cat| %>
    <tr>
        <td><%= cat.name%></td>
    </tr> 
    <% end %>
</table>

<h2> Users </h2>
<table>
	<tr>
		<td>Username</td>
		<td>Email</td>
		<td>Age</td>
	</tr>
	<% @user.each do |user| %>
    <tr>
      <td><%= user.username%></td>
      <td><%= user.email%></td>
      <td><%= user.age%></td>
    </tr>
  <% end %>
</table>

<h2> Todos </h2>
<table>
	<tr>
		<td>Tasks</td>
		<td>Finished</td>
	</tr>
	<% @todo.each do |todo| %>
    <tr>
      <td><%= todo.tasks%></td>
      <td><%= todo.finished%></td>
    </tr>
  <% end %>
</table>


